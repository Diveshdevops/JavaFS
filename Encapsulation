package pack;

class Encaps{
	private String name;
	public String getName() {
		return name;
	}
	
	public void setName(String name) {
		this.name=name;
	}
	
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age=age;
	}
	
	public int getRoll() {
		return roll;
	}
	public void setRoll(int roll) {
		this.roll=roll;
	}
	
	private int age;
	private int  roll;
}


public class EncapsulateHome {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Encaps sc=new Encaps();
		sc.setName("sanjai");
		sc.setAge(30);
		sc.setRoll(123);
		System.out.println("Name:"+ sc.getName());
		System.out.println("Age:"+ sc.getAge());
		System.out.println("Roll:"+ sc.getRoll());
		
	}

}
